{
  "manifest_version": 2,

  "name": "Nona Placeholder Embedder",
  "description": "This extension replaces anon image placeholders with the images they point to.",
  "version": "2.1",

  "permissions": [
    "http://www.dreamwidth.org/"
  ],
  
  "content_scripts": [
    {
      "matches": ["http://*.dreamwidth.org/*"],
      "js": ["getElementsByClassName-1.0.1.js", "nona.js"],
      "run_at": "document_end"
    }
  ],
  
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
